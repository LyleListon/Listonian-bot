# Environment (development, production, test)
ENVIRONMENT=development

# Blockchain Providers
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_API_KEY
BSC_RPC_URL=https://bsc-dataseed.binance.org/
BASE_RPC_URL=https://mainnet.base.org

# Wallet Configuration
WALLET_PRIVATE_KEY=your_private_key_here
WALLET_ADDRESS=your_wallet_address_here

# API Keys
INFURA_API_KEY=your_infura_api_key
ALCHEMY_API_KEY=your_alchemy_api_key

# MEV Protection
FLASHBOTS_RELAY_URL=https://relay.flashbots.net/

# Flash Loan Providers
AAVE_LENDING_POOL_ADDRESS=0x7d2768dE32b0b80b7a3454c06BdAc94A69DDc7A9

# Bot Configuration
MIN_PROFIT_THRESHOLD=0.5
MAX_SLIPPAGE=1.0
GAS_PRICE_MULTIPLIER=1.1
MAX_TRADE_AMOUNT=1.0
TRADING_ENABLED=false

# Dashboard Configuration
DASHBOARD_PORT=8080
API_PORT=8000
ENABLE_AUTHENTICATION=false
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secure_password_here

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log

# MCP Servers
MCP_SERVER_1_HOST=localhost
MCP_SERVER_1_PORT=9001
MCP_SERVER_1_API_KEY=your_api_key_here
MCP_SERVER_1_ENABLED=false

MCP_SERVER_2_HOST=localhost
MCP_SERVER_2_PORT=9002
MCP_SERVER_2_API_KEY=your_api_key_here
MCP_SERVER_2_ENABLED=false
